page {
  height: 100%;
  background: #A3D1D1;
}

.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.avatar {
  width: 300rpx;
  height: 300rpx;
  border-radius: 100rpx;
  margin: 100rpx 0;
}

.welcome {
  font-size: 32rpx;
  font-weight: bold;
  margin: 50rpx 0;
}

.goAhead {
  width: 200rpx;
  height: 80rpx;
  font-size: 28rpx;
  border: 1rpx solid black;
  border-radius: 20rpx;
  text-align: center;
  line-height: 80rpx;
  margin: 50rpx 0;
}
